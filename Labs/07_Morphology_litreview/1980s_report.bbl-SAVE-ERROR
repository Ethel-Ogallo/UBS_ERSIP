      \strng{namehash}{a944b891fb7930feee0cb337611dca99}
      \strng{fullhash}{a944b891fb7930feee0cb337611dca99}
      \strng{fullhashraw}{a944b891fb7930feee0cb337611dca99}
      \strng{bibnamehash}{a944b891fb7930feee0cb337611dca99}
      \strng{authorbibnamehash}{a944b891fb7930feee0cb337611dca99}
      \strng{authornamehash}{a944b891fb7930feee0cb337611dca99}
