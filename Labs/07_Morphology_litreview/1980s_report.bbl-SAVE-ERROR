      \strng{namehash}{e15244f3cb4920efd6fae1a8d4f15f00}
      \strng{fullhash}{e15244f3cb4920efd6fae1a8d4f15f00}
      \strng{fullhashraw}{e15244f3cb4920efd6fae1a8d4f15f00}
      \strng{bibnamehash}{e15244f3cb4920efd6fae1a8d4f15f00}
